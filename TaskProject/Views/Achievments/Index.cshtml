@model List<TaskProject.Models.Attainment>

<div class="container">
    @foreach(var Attainmeint in Model)
    {
        <div class="col-xs-6 col-sm-4 col-lg-3 sw_attainment">
            <div class="sw_attainment_img" data-title="@Attainmeint.Description"><img src="@Attainmeint.LinkImage" alt=""/></div>
            <span>@Attainmeint.Name</span>
        </div>
    }
</div>